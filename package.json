{
  "name": "pepetd.github.io",
  "version": "1.0.0",
  "description": "website for felipenogueira3d.com",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "run-p clean start:webpack start:hugo",
    "start:hugo": "wait-on data/manifest.json && hugo server -D -E -F --port 8080 --bind 0.0.0.0 --renderToDisk --disableFastRender -DF",
    "start:webpack": "webpack --watch --mode development",
    "build": "run-s clean build:webpack build:hugo",
    "build:hugo": "hugo --minify",
    "build:webpack": "webpack --mode production",
    "clean": "rm -rf public resources static/css static/js static/images data/manifest.json"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "intersection-observer": "^0.11.0",
    "lunr": "^2.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^6.0.3",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "mini-css-extract-plugin": "^1.1.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0",
    "webpack-fix-style-only-entries": "^0.6.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
