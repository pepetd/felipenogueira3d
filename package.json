{
  "name": "pepetd.github.io",
  "version": "1.0.0",
  "description": "website for felipenogueira3d.com",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "run-p clean start:webpack start:hugo",
    "start:hugo": "wait-on data/manifest.json && hugo server -D -E -F --port 8080 --bind 0.0.0.0 --renderToDisk --disableFastRender -DF",
    "start:webpack": "webpack --watch --mode development --display-entrypoints false --hide-modules true",
    "build": "run-s clean build:webpack build:hugo",
    "build:hugo": "hugo --minify",
    "build:webpack": "webpack --mode production --display-entrypoints false --hide-modules true",
    "clean": "rm -rf public resources static/css static/js static/images data/manifest.json"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "intersection-observer": "^0.7.0",
    "lunr": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/free-regular-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-fix-style-only-entries": "^0.3.1",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
